<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= article.body %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= image_tag article.image_url, height: '200px' if article.image.file.present? %>
  </p>
</div>
